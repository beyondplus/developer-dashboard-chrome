{
  "manifest_version": 2,

  "name": "Developer Dashboard",
  "description": "This extension will collect IT news without browse and Developer Dashboard included.",
  "version": "1.1.4",
  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "icons":{
    "64" : "icon.png",
    "32" : "icon.png",
    "16" : "icon.png",
    "128" : "icon.png"
  },
 "chrome_url_overrides":{
    "newtab": "newtab.html"
  },
  "permissions": [
   "activeTab",
   "https://ajax.googleapis.com/ajax/libs/jquery/2.2.0/jquery.min.js"
   ]
}